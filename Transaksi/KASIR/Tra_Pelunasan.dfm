  object Browse_Tagihan_AR_Krd: TUniQuery
    Connection = _DataModule.UniErp_DEPO
    SQL.Strings = (
      
        'select r.NO_TRANSAKSI,SUBSTRING(r.no_transaksi,8,len(r.no_transa' +
        'ksi)-7) as No_urut,'
      '         r.KD_OUTLET,r.debet-ISNULL(r.kredit,0) as saldo,'
      
        '       r.tanggal_transaksi as tgl_transaksi,r.jenis_pembayaran a' +
        's jenis_bayar,r.tgl_jatuh_tempo,r.depo,'
      'w.NAMA as nama_outlet,w.ALAMAT_1,'
      
        '       w.penagih_nik,g.NAMA as nama_penagih,case jenis_pembayara' +
        'n when '#39'TUNAI'#39' then '#39'NOTA TUNAI'#39' else '#39'NOTA KREDIT'#39' end as asal ' +
        ' from ('
      
        'select a.NO_TRANSAKSI,a.KD_OUTLET, a.debet,(select SUM(h.kredit)' +
        ' from Rekap_AR h'
      
        '                                            where h.No_Nota=a.No' +
        '_Nota ) as kredit,'
      
        '      a.tanggal_transaksi,a.jenis_pembayaran,a.tgl_jatuh_tempo,a' +
        '.depo'
      '  from Rekap_AR a '
      
        '       ) r left outer join  MST_SUPPLIER w on w.kode=r.KD_OUTLET' +
        ' '
      
        '           left outer join tb_hris.dbo.m_karyawan g on g.NIK=w.p' +
        'enagih_nik and g.KODE=r.DEPO '
      '       where r.debet-isnull(r.kredit,0)>0;')
    Left = 81
    Top = 357
