    SQL.Strings = (
      
        'SELECT b.ALAMAT_1,c.NAMA as penagih_nama,d.kode_outlet,b.NAMA as' +
        ' nama_outlet,b.TELP_1,d.saldo,D.POTONGAN,   a.*,d.jenis_bayar'
      '  FROM pelunasan_M a'
      
        '  left outer join pelunasan_d  d on a.no_transaksi=d.no_transaks' +
        'i  '
      
        '  left outer join MST_SUPPLIER b on d.kode_outlet=b.KODE and a.d' +
        'epo=b.depo'
      
        '  left outer join TB_HRIS.dbo.M_KARYAWAN c on a.kode_penagih=c.N' +
        'IK and a.depo=c.kode'
