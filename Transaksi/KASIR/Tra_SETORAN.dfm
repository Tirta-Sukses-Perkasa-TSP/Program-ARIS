  object Zbrowse_detil: TUniQuery
    Connection = _DataModule.UniErp_DEPO
    SQL.Strings = (
      
        'SELECT a.depo,a.ID_TRANSAKSI,a.NO_TRANSAKSI,a.STATUS,a.KODE_SALE' +
        'S,0 AS Nilai_Tunai,'#39'GIRO'#39' AS JENIS,'
      
        '       c.NAMA as sales_nama,d.KODE,d.NAMA_BANK,b.NILAI as Nilai_' +
        'Giro,b.no_GIRO,'
      
        '       b.TGL_JATUH_TEMPO,b.ATAS_NAMA,b.KD_OUTLET,e.NAMA as nama_' +
        'outlet,'
      '       e.ALAMAT_1  '
      '  FROM [setoran_M] a '
      '  left outer join SETORAN_D b on a.NO_TRANSAKSI=b.NO_TRANSAKSI '
      '  left outer join TB_HRIS.dbo.MST_BANK d on b.KODE_BANK=d.KODE'
      
        '  left outer join MST_OUTLET e on b.KD_OUTLET=e.KODE and a.DEPO=' +
        'e.depo'
      
        '  left outer join TB_HRIS.dbo.M_KARYAWAN c on a.kode_sales=c.NIK' +
        ' and a.depo=c.kode'
      'where  a.TGL_TRANSAKSI>=:tgl1'
      '  and  a.TGL_TRANSAKSI<=:tgl2'
      ''
      '  '
      'union all'
      
        'SELECT a.depo,a.ID_TRANSAKSI,a.NO_TRANSAKSI,a.STATUS,a.KODE_SALE' +
        'S,a.total_tunai AS Tunai,'#39'TUNAI'#39' AS JENIS,'
      '       c.NAMA as sales_nama,'#39#39','#39#39',0,'#39#39','
      '       '#39#39','#39#39','#39#39','#39#39','
      '       '#39#39'  '
      '  FROM [setoran_M] a '
      
        '  left outer join TB_HRIS.dbo.M_KARYAWAN c on a.kode_sales=c.NIK' +
        ' and a.depo=c.kode'
      'where  a.TGL_TRANSAKSI>=:tgl1'
      '  and  a.TGL_TRANSAKSI<=:tgl2')
    Left = 777
    Top = 421
    ParamData = <
      item
        DataType = ftDateTime
        Name = 'tgl1'
        ParamType = ptInput
        Value = 40909d
      end
      item
        DataType = ftDateTime
        Name = 'tgl2'
        ParamType = ptInput
        Value = 41640d
      end>
