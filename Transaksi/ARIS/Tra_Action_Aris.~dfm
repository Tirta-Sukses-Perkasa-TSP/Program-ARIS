    SQLInsert.Strings = (
      'INSERT INTO action_aris_m'
      
        '  (ID_TRANSAKSI, KODE, NO_TRANSAKSI, KETERANGAN, NAMA_TTD1, TGL_' +
        'TTD1, TGL_TRANSAKSI, OPR_INSERT, TGL_INSERT, OPR_EDIT, TGL_EDIT,' +
        ' OPR_TERMINATED, TGL_TERMINATED, REASON_TERMINATED, STATUS, OLD_' +
        'STATUS, BAGIAN, NAMA_TTD2, NAMA_TTD3, NAMA_TTD4, TGL_TTD2, TGL_T' +
        'TD3, TGL_TTD4, NIK)'
      'VALUES'
      
        '  (:ID_TRANSAKSI, :KODE, :NO_TRANSAKSI, :KETERANGAN, :NAMA_TTD1,' +
        ' :TGL_TTD1, :TGL_TRANSAKSI, :OPR_INSERT, :TGL_INSERT, :OPR_EDIT,' +
        ' :TGL_EDIT, :OPR_TERMINATED, :TGL_TERMINATED, :REASON_TERMINATED' +
        ', :STATUS, :OLD_STATUS, :BAGIAN, :NAMA_TTD2, :NAMA_TTD3, :NAMA_T' +
        'TD4, :TGL_TTD2, :TGL_TTD3, :TGL_TTD4, :NIK)')
    SQLDelete.Strings = (
      'DELETE FROM action_aris_m'
      'WHERE'
      '  NO_TRANSAKSI = :Old_NO_TRANSAKSI')
    SQLUpdate.Strings = (
      'UPDATE action_aris_m'
      'SET'
      
        '  ID_TRANSAKSI = :ID_TRANSAKSI, KODE = :KODE, NO_TRANSAKSI = :NO' +
        '_TRANSAKSI, KETERANGAN = :KETERANGAN, NAMA_TTD1 = :NAMA_TTD1, TG' +
        'L_TTD1 = :TGL_TTD1, TGL_TRANSAKSI = :TGL_TRANSAKSI, OPR_INSERT =' +
        ' :OPR_INSERT, TGL_INSERT = :TGL_INSERT, OPR_EDIT = :OPR_EDIT, TG' +
        'L_EDIT = :TGL_EDIT, OPR_TERMINATED = :OPR_TERMINATED, TGL_TERMIN' +
        'ATED = :TGL_TERMINATED, REASON_TERMINATED = :REASON_TERMINATED, ' +
        'STATUS = :STATUS, OLD_STATUS = :OLD_STATUS, BAGIAN = :BAGIAN, NA' +
        'MA_TTD2 = :NAMA_TTD2, NAMA_TTD3 = :NAMA_TTD3, NAMA_TTD4 = :NAMA_' +
        'TTD4, TGL_TTD2 = :TGL_TTD2, TGL_TTD3 = :TGL_TTD3, TGL_TTD4 = :TG' +
        'L_TTD4, NIK = :NIK'
      'WHERE'
      '  NO_TRANSAKSI = :Old_NO_TRANSAKSI')
    SQLLock.Strings = (
      'SELECT * FROM action_aris_m'
      'WITH (UPDLOCK, ROWLOCK, HOLDLOCK)'
      'WHERE'
