  object UniQuery1: TUniQuery
    SQLInsert.Strings = (
      'INSERT INTO ARIS_D'
      
        '  (DESKRIPSI_PERMINTAAN, tANGGAL_DEADLINE, NO_TRANSAKSI, Ditanga' +
        'ni, ID_Transaksi_action, Keterangan, is_ditangani, Sts_Action, k' +
        'ode_item, qty, qty_action, Status_Permintaan)'
      'VALUES'
      
        '  (:DESKRIPSI_PERMINTAAN, :tANGGAL_DEADLINE, :NO_TRANSAKSI, :Dit' +
        'angani, :ID_Transaksi_action, :Keterangan, :is_ditangani, :Sts_A' +
        'ction, :kode_item, :qty, :qty_action, :Status_Permintaan)'
      'SET :Id_transaksi_D = SCOPE_IDENTITY()')
    SQLDelete.Strings = (
      'DELETE FROM ARIS_D'
      'WHERE'
      '  Id_transaksi_D = :Old_Id_transaksi_D')
    SQLUpdate.Strings = (
      'UPDATE ARIS_D'
      'SET'
      
        '  DESKRIPSI_PERMINTAAN = :DESKRIPSI_PERMINTAAN, tANGGAL_DEADLINE' +
        ' = :tANGGAL_DEADLINE, NO_TRANSAKSI = :NO_TRANSAKSI, Ditangani = ' +
        ':Ditangani, ID_Transaksi_action = :ID_Transaksi_action, Keterang' +
        'an = :Keterangan, is_ditangani = :is_ditangani, Sts_Action = :St' +
        's_Action, kode_item = :kode_item, qty = :qty, qty_action = :qty_' +
        'action, Status_Permintaan = :Status_Permintaan'
      'WHERE'
      '  Id_transaksi_D = :Old_Id_transaksi_D')
    SQLLock.Strings = (
      'SELECT * FROM ARIS_D'
      'WITH (UPDLOCK, ROWLOCK, HOLDLOCK)'
      'WHERE'
      '  Id_transaksi_D = :Old_Id_transaksi_D')
    SQLRefresh.Strings = (
      
        'SELECT DESKRIPSI_PERMINTAAN, tANGGAL_DEADLINE, NO_TRANSAKSI, Dit' +
        'angani, ID_Transaksi_action, Keterangan, is_ditangani, Sts_Actio' +
        'n, kode_item, qty, qty_action, Status_Permintaan FROM ARIS_D'
      'WHERE'
      '  Id_transaksi_D = :Id_transaksi_D')
    DataTypeMap = <>
    Connection = _DataModule.UniFirebird
    SQL.Strings = (
      '/****** Script for SelectTopNRows command from SSMS  ******/'
      'SELECT coalesce(max(Id_transaksi_D),0)+1 as nomer'
      '  FROM [tb_hris].[dbo].[action_aris_d]')
    BeforeInsert = ZMasterBeforeDelete
    AfterPost = ZDetilAfterPost
    OnNewRecord = ZdetilNewRecord
    Left = 584
    Top = 221
    object UniQuery1nomer: TIntegerField
      FieldName = 'nomer'
      ReadOnly = True
    end
  end
