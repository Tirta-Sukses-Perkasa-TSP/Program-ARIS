      '  where a.kode_wilayah=b.kode_wilayah'
      #9' and  ISNULL(Internal_Kecil,'#39'-'#39')<>'#39'-'#39
      'union all'
      'SELECT a.Plant,a.kode_shipto'
      '        ,a.tujuan'
      '        ,a.kode_wilayah'
      '      ,a.alamat_shipto'
      '      ,Internal_besar as Route_Kode'
      #9'  ,'#39'Internal_besar'#39' as Keterangan,Tujuan_Kirim'
      '     FROM mars.dbo.sc_mst_shipto a,mars.dbo.sc_mst_wilayah b'
      '  where a.kode_wilayah=b.kode_wilayah'
      #9'and ISNULL(Internal_besar,'#39'-'#39')<>'#39'-'#39
      'union all'
      'SELECT a.Plant,a.kode_shipto'
      '        ,a.tujuan'
      '        ,a.kode_wilayah'
      '      ,a.alamat_shipto'
      '      ,Ambil_Sendiri  as Route_Kode'
      #9'  ,'#39'Ambil_Sendiri'#39' as Keterangan,Tujuan_Kirim'
      '    FROM mars.dbo.sc_mst_shipto a,mars.dbo.sc_mst_wilayah b'
      '  where a.kode_wilayah=b.kode_wilayah'
      #9'and ISNULL(Ambil_Sendiri,'#39'-'#39')<>'#39'-'#39
      'union all'
      'SELECT a.Plant,a.kode_shipto'
      '        ,a.tujuan'
      '        ,a.kode_wilayah'
      '      ,a.alamat_shipto'
      '      ,Sewa  as Route_Kode'
      #9'  ,'#39'Sewa'#39' as Keterangan,Tujuan_Kirim'
      '  FROM mars.dbo.sc_mst_shipto a,mars.dbo.sc_mst_wilayah b'
      '  where a.kode_wilayah=b.kode_wilayah'
      #9'and ISNULL( Sewa,'#39'-'#39')<>'#39'-'#39
      'union all'
      'SELECT a.Plant,a.kode_shipto'
      '        ,a.tujuan'
      '        ,a.kode_wilayah'
      '        ,a.alamat_shipto'
      '      ,Container  as Route_Kode'
      #9'  ,'#39'Container'#39' as Keterangan,Tujuan_Kirim'
      '  FROM mars.dbo.sc_mst_shipto a,mars.dbo.sc_mst_wilayah b'
      '  where a.kode_wilayah=b.kode_wilayah'
      #9'and ISNULL(Container,'#39'-'#39')<>'#39'-'#39
      #9') r,mars.dbo.SAP_YM137 g'
      #9'where PONumber=:0'
      #9' and  r.kode_shipto=  cast(g.CustomerNo1 as int)'
      ''
      
        ' group by r.Plant,r.kode_shipto,r.tujuan,r.kode_wilayah,r.alamat' +
