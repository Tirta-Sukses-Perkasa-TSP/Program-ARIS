  object List_PO: TUniQuery
    DataTypeMap = <>
    Connection = _DataModule.UniConnection_Mars
    SQL.Strings = (
      'select t.*,'
      
        '(select count(*) from SC_TRA_Plan_Kiriman_D h where h.PONumber=t' +
        '.PONumber and h.Delivered_date=t.DeliveryDate and h.Material_cd=' +
        't.Material_cd) as jum_kend'
      ' from ('
      
        'select  q.Material_cd,q.Material_Nama,q.Group_Material,q.Brand,q' +
        '.JENIS,q.ItemText,q.HeaderText,'
      
        '        c.branch as plant,c.KETERANGAN as Plant_Nama_TMP,q.PODAT' +
        'E,q.DeliveryDate,'
      #9'    POQty,q.PONumber,q.Customer,q.CustomerName,'
      '           isnull((SELECT sum(deliver_qty)'
      '                      FROM mars.dbo.SC_Delivery a'
      '                      where a.po_number=q.ponumber'
      
        '                      and material_code=q.Material_cd ),0) as Qt' +
        'y_Delivery'
      '         ,c.SUPPORTED_BRANCH as SUPPORTED_BRANCH'
      '      '#9'   ,c.Nama_Origin as Nama_Origin'
      #9#9'from ('
      
        'SELECT b.Material_cd,b.Material_Nama,b.Group_Material,b.Brand,b.' +
        'JENIS,a.Vendor,a.PODATE,'
      '       a.POQty, DeliveryDate'
      
        '       ,a.PONumber,a.Customer,a.CustomerName,a.ItemText,a.Header' +
        'Text'
      '        '
      ' '
      '  FROM mars.dbo. SAP_YM137 a , mars.dbo.mst_material b'
      
        '  where  CAST(a.MATERIAL AS INT) =b.Material_cd) q inner join  G' +
        'R_LIST_REKANAN c'
      '  on q.vendor=c.kode_Sap'
      '  where  (c.GROUPING = '#39'TSP'#39')'
      '    and DeliveryDate=:0'
      '  /*  and POQty-isnull((SELECT sum(deliver_qty)'
      '                      FROM mars.dbo.SC_Delivery a'
      '                      where a.po_number=q.ponumber'
      '                      and material_code=q.Material_cd ),0)>0'
      '   */'
      ' --   and ponumber='#39'4601623255'#39
      ') t')
