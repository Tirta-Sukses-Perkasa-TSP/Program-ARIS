  object Qstock: TUniQuery
    DataTypeMap = <>
    Connection = _DataModule.UniConnection_Mars
    SQL.Strings = (
      '/****** Script for SelectTopNRows command from SSMS  ******/'
      'SELECT'
      '       a.material_code,a.material_name,OH_Date,'
      '       SKU_GROUP,sum(Unrestricted) as Stock '
      #9'   ,'
      #9'    isnull( (SELECT  sum(f.Qty_kirim)'
      
        '  FROM mars.dbo.SC_TRA_Plan_Kiriman_D f,mars.dbo.SC_TRA_Plan_Kir' +
        'iman_m g'
      '  where f.no_transaksi=g.no_transaksi'
      '   and f.Material_cd=a.material_code'
      '   and f.Delivered_date=:0'
      '   and g.Plant=a.plant'
      '  ) ,0) as Qty_Planning_Pengiriman'
      ','
      '  isnull(( SELECT sum(qty_Plan)'
      ' FROM prd_TRA_plp_D h,PRD_TRA_PLP_M j'
      '  where h.NO_TRANSAKSI=j.NO_TRANSAKSI'
      '    and j.BRANCH=a.plant'
      #9'and j.TGL_TRANSAKSI=:0'
      #9'-- and j.TGL_TRANSAKSI<=dateadd(day,1,:0)'
      #9'and h.material_code=a.material_code'
      #9'),0) as Qty_Planning_Produksi'
      '  FROM mars.dbo.sc_stock a,mst_material b'
      '  where plant=:1'
      '    and a.material_code=b.Material_cd'
      #9'and b.JENIS='#39'FG'#39
      
        #9'and a.OH_Date=dateadd(day,(case when DATEPART(dw,:0)=2 then 2 e' +
        'lse 1 end)*-1 ,:0)'
      #9'and Unrestricted>0'
      
        'group by  a.material_code,a.material_name,OH_Date, SKU_GROUP,pla' +
        'nt'
      'order by SKU_GROUP,a.material_code')
    OnCalcFields = QstockCalcFields
