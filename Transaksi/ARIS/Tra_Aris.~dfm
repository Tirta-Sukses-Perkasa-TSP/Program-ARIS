      'INSERT INTO aris_d'
      
        '  (DESKRIPSI_PERMINTAAN, tANGGAL_DEADLINE, NO_TRANSAKSI, Ditanga' +
        'ni, ID_Transaksi_action, Keterangan, is_ditangani, Sts_Action)'
