  object Total_detil: TUniQuery
    KeyFields = 'id_transaksi_d'
    SQLInsert.Strings = (
      'INSERT INTO NOTA_JUAL_D'
      
        '  (NO_TRANSAKSI, KODE_ITEM, QTY, HARGA, GROSS, qty_kirim, no_tra' +
        'nsaksi_lama, qty_kembali, keterangan)'
      'VALUES'
      
        '  (:NO_TRANSAKSI, :KODE_ITEM, :QTY, :HARGA, :GROSS, :qty_kirim, ' +
        ':no_transaksi_lama, :qty_kembali, :keterangan)'
      'SET :ID_TRANSAKSI_D = SCOPE_IDENTITY()')
    SQLDelete.Strings = (
      'DELETE FROM NOTA_JUAL_D'
      'WHERE'
      '  ID_TRANSAKSI_D = :Old_ID_TRANSAKSI_D')
    SQLUpdate.Strings = (
      'UPDATE NOTA_JUAL_D'
      'SET'
      
        '  NO_TRANSAKSI = :NO_TRANSAKSI, KODE_ITEM = :KODE_ITEM, QTY = :Q' +
        'TY, HARGA = :HARGA, GROSS = :GROSS, qty_kirim = :qty_kirim, no_t' +
        'ransaksi_lama = :no_transaksi_lama, qty_kembali = :qty_kembali, ' +
        'keterangan = :keterangan'
      'WHERE'
      '  ID_TRANSAKSI_D = :Old_ID_TRANSAKSI_D')
    SQLLock.Strings = (
      'SELECT * FROM NOTA_JUAL_D'
      'WITH (UPDLOCK, ROWLOCK, HOLDLOCK)'
      'WHERE'
      '  ID_TRANSAKSI_D = :Old_ID_TRANSAKSI_D')
    SQLRefresh.Strings = (
      
        'SELECT NO_TRANSAKSI, KODE_ITEM, QTY, HARGA, GROSS, qty_kirim, no' +
        '_transaksi_lama, qty_kembali, keterangan FROM NOTA_JUAL_D'
      'WHERE'
      '  ID_TRANSAKSI_D = :ID_TRANSAKSI_D')
    Connection = _DataModule.UniErp_DEPO
