  object Zdetil: TUniQuery [16]
    KeyFields = 'id_transaksi_d'
    SQLInsert.Strings = (
      'INSERT INTO sppb_d'
      
        '  (NO_TRANSAKSI, NO_TRANSAKSI_NOTA, ID_TRANSAKSI_D_NOTA, KODE_IT' +
        'EM, QTY, QTY_BTB, JENIS_NOTA)'
      'VALUES'
      
        '  (:NO_TRANSAKSI, :NO_TRANSAKSI_NOTA, :ID_TRANSAKSI_D_NOTA, :KOD' +
        'E_ITEM, :QTY, :QTY_BTB, :JENIS_NOTA)'
      'SET :ID_TRANSAKSI_D = SCOPE_IDENTITY()')
    SQLDelete.Strings = (
      'DELETE FROM sppb_d'
      'WHERE'
      '  ID_TRANSAKSI_D = :Old_ID_TRANSAKSI_D')
    SQLUpdate.Strings = (
      'UPDATE sppb_d'
      'SET'
      
        '  NO_TRANSAKSI = :NO_TRANSAKSI, NO_TRANSAKSI_NOTA = :NO_TRANSAKS' +
        'I_NOTA, ID_TRANSAKSI_D_NOTA = :ID_TRANSAKSI_D_NOTA, KODE_ITEM = ' +
        ':KODE_ITEM, QTY = :QTY, QTY_BTB = :QTY_BTB, JENIS_NOTA = :JENIS_' +
        'NOTA'
      'WHERE'
      '  ID_TRANSAKSI_D = :Old_ID_TRANSAKSI_D')
    SQLLock.Strings = (
      'SELECT * FROM sppb_d'
      'WITH (UPDLOCK, ROWLOCK, HOLDLOCK)'
      'WHERE'
      '  ID_TRANSAKSI_D = :Old_ID_TRANSAKSI_D')
    SQLRefresh.Strings = (
      
        'SELECT NO_TRANSAKSI, NO_TRANSAKSI_NOTA, ID_TRANSAKSI_D_NOTA, KOD' +
        'E_ITEM, QTY, QTY_BTB, JENIS_NOTA FROM sppb_d'
      'WHERE'
      '  ID_TRANSAKSI_D = :ID_TRANSAKSI_D')
    Connection = _DataModule.UniErp_DEPO
    SQL.Strings = (
      'select a.*'
      
        '  ,(select b.NO_TRANSAKSI from  TB_ARIS_KLM.dbo.NOTA_JUAL_M b wh' +
        'ere b.NO_TRANSAKSI=a.NO_TRANSAKSI) as NO_ASET'
      
        '  ,case jenis_nota when '#39'NOTA FOC'#39' THEN (select b.KD_OUTLET from' +
        '  NOTA_JUAL_M_foc b where b.NO_TRANSAKSI=a.NO_TRANSAKSI_NOTA)'
      
        '  else (select b.KD_OUTLET from  NOTA_JUAL_M b where b.NO_TRANSA' +
        'KSI=a.NO_TRANSAKSI_NOTA) end  as kd_outlet  '
      'from sppb_d  a'
      'where no_transaksi=:no_transaksi')
    MasterSource = Datasource1
    MasterFields = 'NO_TRANSAKSI'
    DetailFields = 'NO_TRANSAKSI'
    BeforeInsert = ZMasterBeforeDelete
    BeforePost = ZdetilBeforePost
    AfterPost = ZDetilAfterPost
    OnNewRecord = ZdetilNewRecord
    Left = 448
    Top = 269
    ParamData = <
      item
        DataType = ftString
        Name = 'NO_TRANSAKSI'
        ParamType = ptInput
      end>
