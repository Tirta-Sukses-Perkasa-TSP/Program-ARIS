    SQL.Strings = (
      
        'SELECT    a.*,b.NAMA as nama_penanggung_jawab,d.NO_POLISI,d.KETE' +
        'RANGAN as kendaraan,'
      '          e.NO_TRANSAKSI_NOTA,f.KD_OUTLET,e.QTY, E.QTY_BTB'
      'FROM sppb_m AS a left outer join'
      
        '     TB_HRIS.dbo.M_KARYAWAN b on a.KODE_PENANGGUNG_JAWAB=b.NIK l' +
        'eft outer join'
      
        '     TB_HRIS.dbo.MST_DIVISI c on a.kode_gudang=c.divisi left out' +
        'er join'
      
        '     TB_ARIS.dbo.MST_kendaraan d on a.KODE_KENDARAAN=d.kode left' +
        ' outer join'
      '     SPPB_D e on a.NO_TRANSAKSI=e.NO_TRANSAKSI left outer join '
      
        '     View_Nota_all f on e.NO_TRANSAKSI_NOTA=f.NO_TRANSAKSI left ' +
        'outer join'
      
        '     TB_ARIS_KLM.dbo.NOTA_JUAL_M h on e.NO_TRANSAKSI=h.NO_TRANSA' +
        'KSI'
      '     '
      'where  a.TGL_TRANSAKSI>=:tgl1'
      '  and  a.TGL_TRANSAKSI<=:tgl2')
