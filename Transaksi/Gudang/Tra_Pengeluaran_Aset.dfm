  object Browse_Permintaan: TUniQuery
    SQLInsert.Strings = (
      'INSERT INTO ASET_M'
      '  (ID_TRANSAKSI, NO_TRANSAKSI, JENIS)'
      'VALUES'
      '  (:ID_TRANSAKSI, :NO_TRANSAKSI, :JENIS)')
    SQLDelete.Strings = (
      'DELETE FROM ASET_M'
      'WHERE'
      '  NO_TRANSAKSI = :Old_NO_TRANSAKSI')
    SQLUpdate.Strings = (
      'UPDATE ASET_M'
      'SET'
      
        '  ID_TRANSAKSI = :ID_TRANSAKSI, NO_TRANSAKSI = :NO_TRANSAKSI, JE' +
        'NIS = :JENIS'
      'WHERE'
      '  NO_TRANSAKSI = :Old_NO_TRANSAKSI')
    SQLLock.Strings = (
      'SELECT * FROM ASET_M'
      'WITH (UPDLOCK, ROWLOCK, HOLDLOCK)'
      'WHERE'
      '  NO_TRANSAKSI = :Old_NO_TRANSAKSI')
    SQLRefresh.Strings = (
      'SELECT ID_TRANSAKSI, NO_TRANSAKSI, JENIS FROM ASET_M'
      'WHERE'
      '  NO_TRANSAKSI = :NO_TRANSAKSI')
    Connection = _DataModule.UniErp_DEPO
    SQL.Strings = (
      '/*'
      
        'SELECT a.id_transaksi,b.is_black_list,a.no_transaksi,d.id_transa' +
        'ksi_d,a.tgl_transaksi,a.tgl_rencana_pengiriman,a.tgl_expired,'
      
        '  a.kd_outlet, b.NAMA,b.ALAMAT_1,f.nama_wilayah,c.NAMA as sales_' +
        'nama, '
      
        '  a.no_sp,d.kode_item,e.NAMA_ITEM,e.SATUAN,d.qty-isnull(d.qty_ki' +
        'rim,0)+isnull(d.qty_kembali,0) as qty_sisa'
      '  ,a.jenis_nota'
      '  FROM [NOTA_JUAL_M] a'
      
        '  left outer join MST_OUTLET b on a.kd_outlet=b.KODE and a.depo=' +
        'b.depo'
      
        '  left outer join TB_HRIS.dbo.M_KARYAWAN c on a.kode_sales=c.NIK' +
        ' and a.depo=c.kode'
      '  inner join [NOTA_JUAL_d] d on a.no_transaksi=d.no_transaksi'
      
        '  inner join [TB_ARIS].[dbo].[MST_ITEM] e on d.kode_item=e.KODE_' +
        'ITEM'
      
        '  left outer join  [TB_ARIS].[dbo].[MST_wilayah] f on b.wilayah=' +
        'f.wilayah'
      
        'where a.status='#39'2'#39' and isnull(b.is_black_list,'#39'F'#39')<>'#39'T'#39' and d.qt' +
        'y-isnull(d.qty_kirim,0)+isnull(d.qty_kembali,0)>0'
      '*/'
      ''
      '/*'
      'SELECT a.ID_TRANSAKSI,a.NO_TRANSAKSI, b.KODE_ITEM, c.DIVISI'
      'FROM ASET_M AS a'
      'LEFT OUTER JOIN ASET_D AS b ON a.NO_TRANSAKSI=b.NO_TRANSAKSI'
      
        'LEFT OUTER JOIN TB_HRIS.DBO.MST_DIVISI AS c ON a.KD_OUTLET=c.DIV' +
        'ISI'
      '*/'
      ''
      
        'SELECT a.ID_TRANSAKSI,a.NO_TRANSAKSI,a.JENIS, b.KODE_ITEM, b.ID_' +
        'TRANSAKSI_D, b.QTY'
      'FROM ASET_M AS a'
      'LEFT OUTER JOIN ASET_D AS b ON a.NO_TRANSAKSI=b.NO_TRANSAKSI')
    Left = 217
    Top = 405
