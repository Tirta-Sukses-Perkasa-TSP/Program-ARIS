      '  ID_TRANSAKSI_D = :Old_ID_TRANSAKSI_D')
    SQLUpdate.Strings = (
      'UPDATE PENGELUARAN_ASET_D'
      'SET'
      
        '  NO_TRANSAKSI = :NO_TRANSAKSI, NO_TRANSAKSI_ASET = :NO_TRANSAKS' +
        'I_ASET, ID_TRANSAKSI_D_ASET = :ID_TRANSAKSI_D_ASET, KODE_ITEM = ' +
        ':KODE_ITEM, QTY = :QTY, QTY_BTB = :QTY_BTB, JENIS_ASET = :JENIS_' +
        'ASET'
      'WHERE'
      '  ID_TRANSAKSI_D = :Old_ID_TRANSAKSI_D')
    SQLLock.Strings = (
