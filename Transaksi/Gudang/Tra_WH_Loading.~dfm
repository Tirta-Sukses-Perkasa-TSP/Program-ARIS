  object Cetak: TUniQuery
    DataTypeMap = <>
    Connection = _DataModule.UniConnection_Mars
    SQL.Strings = (
      'use mars'
      '/****** Script for SelectTopNRows command from SSMS  ******/'
      
        'SELECT a.NO_TRANSAKSI,max(a.KETERANGAN) as keterangan_master,a.T' +
        'GL_TRANSAKSI,a.BRANCH,'
      '       b.material_code,b.ID_MESIN, c.kode_mesin,d.Material_Nama,'
      
        #9'   sum(b.qty_FG) as qty_FG,max(b.keterangan) as Keterangan_deti' +
        'l'
      
        '  FROM mars.dbo.PRD_TRA_ST_Produksi_M a,mars.dbo.prd_tra_st_prod' +
        'uksi_d b,'
      '       mst_mesin c,mst_material d'
      '  where a.NO_TRANSAKSI=:0'
      '  and a.NO_TRANSAKSI=b.no_transaksi'
      '  and b.ID_MESIN=c.id_mesin'
      '  and b.material_code=d.Material_cd'
      'group by a.NO_TRANSAKSI,a.TGL_TRANSAKSI,a.BRANCH,'
      '       b.material_code,b.ID_MESIN, c.kode_mesin,d.Material_Nama')
    Left = 502
    Top = 361
