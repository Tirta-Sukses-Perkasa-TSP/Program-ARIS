      ' FROM [mars].[dbo].[mst_material_rm]'
      ' '
      ''
      ' union All'
      ' select Jenis,Jenis,'#39'-'#39',1,'#39'Status'#39
      ' from mars.dbo.mst_status_mesin) r'
      'order by r.urut')
