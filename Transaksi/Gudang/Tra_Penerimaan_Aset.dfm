  object UpdateMaster: TUniQuery
    SQLInsert.Strings = (
      'INSERT INTO penerimaan_aset_d'
      
        '  (NO_TRANSAKSI, KODE_ITEM, QTY, NO_TRANSAKSI_PO, KODE_SUPPLIER,' +
        ' KODE_ASET, QTY_TERIMA, id_transaksi_PO, NO_ASET, keterangan, ha' +
        'rga, discount, ppn, jenis_ppn, total)'
      'VALUES'
      
        '  (:NO_TRANSAKSI, :KODE_ITEM, :QTY, :NO_TRANSAKSI_PO, :KODE_SUPP' +
        'LIER, :KODE_ASET, :QTY_TERIMA, :id_transaksi_PO, :NO_ASET, :kete' +
        'rangan, :harga, :discount, :ppn, :jenis_ppn, :total)'
      'SET :ID_TRANSAKSI_D = SCOPE_IDENTITY()')
    SQLDelete.Strings = (
      'DELETE FROM penerimaan_aset_d'
      'WHERE'
      '  ID_TRANSAKSI_D = :Old_ID_TRANSAKSI_D')
    SQLUpdate.Strings = (
      'UPDATE penerimaan_aset_d'
      'SET'
      
