    SQL.Strings = (
      'SELECT    a.*,b.NAMA as nama_penanggung_jawab,'
      '          e.NO_TRANSAKSI_aset,e.QTY, E.QTY_BTB,'
      '          f.NAMA as nama_supplier,f.ALAMAT_1,'
      '          g.KODE_ITEM,g.NAMA_ITEM'
      'FROM PENGELUARAN_ASET_M AS a left outer join'
      
        '     TB_HRIS.dbo.M_KARYAWAN b on a.KODE_PENANGGUNG_JAWAB=b.NIK l' +
        'eft outer join'
      
        '     TB_HRIS.dbo.MST_DIVISI c on a.kode_gudang=c.divisi left out' +
        'er join'
      
        '     TB_HRIS.dbo.MST_BAGIAN d on a.KODE_BAGIAN=d.BAGIAN left out' +
        'er join'
      
        '     PENGELUARAN_ASET_D e on a.NO_TRANSAKSI=e.NO_TRANSAKSI left ' +
        'outer join'
      '     MST_SUPPLIER f on f.kode=a.kode_supplier left outer join'
      '     tb_aris.dbo.MST_ITEM g on g.KODE_ITEM=e.KODE_ITEM'
      '     '
      'where  a.TGL_TRANSAKSI>=:tgl1'
      '  and  a.TGL_TRANSAKSI<=:tgl2'
      ''
      'ORDER BY a.ID_TRANSAKSI')
    Left = 457
    Top = 325
