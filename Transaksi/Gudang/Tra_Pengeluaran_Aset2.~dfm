  object Browse_Item: TUniQuery
    SQLInsert.Strings = (
      'INSERT INTO MST_Item'
      
        '  (KODE_ITEM, NAMA_ITEM, SINGK_ITEM, BRAND, KD_GROUP, UKURAN, VO' +
        'LUME, BERAT, KLASIFIKASI, MINIMUM_STOCK, IS_AKTIF, TGL_NON_AKTIF' +
        ', barcode, SATUAN, JADWAL_PM)'
      'VALUES'
      
        '  (:KODE_ITEM, :NAMA_ITEM, :SINGK_ITEM, :BRAND, :KD_GROUP, :UKUR' +
        'AN, :VOLUME, :BERAT, :KLASIFIKASI, :MINIMUM_STOCK, :IS_AKTIF, :T' +
        'GL_NON_AKTIF, :barcode, :SATUAN, :JADWAL_PM)')
    SQLDelete.Strings = (
      'DELETE FROM MST_Item'
      'WHERE'
      '  KODE_ITEM = :Old_KODE_ITEM')
    SQLUpdate.Strings = (
      'UPDATE MST_Item'
      'SET'
      
        '  KODE_ITEM = :KODE_ITEM, NAMA_ITEM = :NAMA_ITEM, SINGK_ITEM = :' +
        'SINGK_ITEM, BRAND = :BRAND, KD_GROUP = :KD_GROUP, UKURAN = :UKUR' +
        'AN, VOLUME = :VOLUME, BERAT = :BERAT, KLASIFIKASI = :KLASIFIKASI' +
        ', MINIMUM_STOCK = :MINIMUM_STOCK, IS_AKTIF = :IS_AKTIF, TGL_NON_' +
        'AKTIF = :TGL_NON_AKTIF, barcode = :barcode, SATUAN = :SATUAN, JA' +
        'DWAL_PM = :JADWAL_PM'
      'WHERE'
      '  KODE_ITEM = :Old_KODE_ITEM')
    SQLLock.Strings = (
      'SELECT * FROM MST_Item'
      'WITH (UPDLOCK, ROWLOCK, HOLDLOCK)'
      'WHERE'
      '  KODE_ITEM = :Old_KODE_ITEM')
    SQLRefresh.Strings = (
      
        'SELECT KODE_ITEM, NAMA_ITEM, SINGK_ITEM, BRAND, KD_GROUP, UKURAN' +
        ', VOLUME, BERAT, KLASIFIKASI, MINIMUM_STOCK, IS_AKTIF, TGL_NON_A' +
        'KTIF, barcode, SATUAN, JADWAL_PM FROM MST_Item'
      'WHERE'
      '  KODE_ITEM = :KODE_ITEM')
    Connection = _DataModule.UniErp
    SQL.Strings = (
      'select *'
      'from MST_Item')
