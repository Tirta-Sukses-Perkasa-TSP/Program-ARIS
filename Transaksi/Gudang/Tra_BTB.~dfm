  object ZBrowse: TUniQuery [13]
    SQLInsert.Strings = (
      'INSERT INTO ACTION_PERMINTAAN_ASET_M'
      
        '  (ID_TRANSAKSI, KODE, NO_TRANSAKSI, KETERANGAN, NAMA_TTD1, TGL_' +
        'TTD1, TGL_TRANSAKSI, OPR_INSERT, TGL_INSERT, OPR_EDIT, TGL_EDIT,' +
        ' OPR_TERMINATED, TGL_TERMINATED, REASON_TERMINATED, STATUS, OLD_' +
        'STATUS, BAGIAN, NAMA_TTD2, NAMA_TTD3, NAMA_TTD4, TGL_TTD2, TGL_T' +
        'TD3, TGL_TTD4, NIK)'
      'VALUES'
      
        '  (:ID_TRANSAKSI, :KODE, :NO_TRANSAKSI, :KETERANGAN, :NAMA_TTD1,' +
        ' :TGL_TTD1, :TGL_TRANSAKSI, :OPR_INSERT, :TGL_INSERT, :OPR_EDIT,' +
        ' :TGL_EDIT, :OPR_TERMINATED, :TGL_TERMINATED, :REASON_TERMINATED' +
        ', :STATUS, :OLD_STATUS, :BAGIAN, :NAMA_TTD2, :NAMA_TTD3, :NAMA_T' +
        'TD4, :TGL_TTD2, :TGL_TTD3, :TGL_TTD4, :NIK)')
    SQLDelete.Strings = (
      'DELETE FROM ACTION_PERMINTAAN_ASET_M'
      'WHERE'
      '  NO_TRANSAKSI = :Old_NO_TRANSAKSI')
    SQLUpdate.Strings = (
      'UPDATE ACTION_PERMINTAAN_ASET_M'
      'SET'
      
        '  ID_TRANSAKSI = :ID_TRANSAKSI, KODE = :KODE, NO_TRANSAKSI = :NO' +
        '_TRANSAKSI, KETERANGAN = :KETERANGAN, NAMA_TTD1 = :NAMA_TTD1, TG' +
        'L_TTD1 = :TGL_TTD1, TGL_TRANSAKSI = :TGL_TRANSAKSI, OPR_INSERT =' +
        ' :OPR_INSERT, TGL_INSERT = :TGL_INSERT, OPR_EDIT = :OPR_EDIT, TG' +
        'L_EDIT = :TGL_EDIT, OPR_TERMINATED = :OPR_TERMINATED, TGL_TERMIN' +
        'ATED = :TGL_TERMINATED, REASON_TERMINATED = :REASON_TERMINATED, ' +
        'STATUS = :STATUS, OLD_STATUS = :OLD_STATUS, BAGIAN = :BAGIAN, NA' +
        'MA_TTD2 = :NAMA_TTD2, NAMA_TTD3 = :NAMA_TTD3, NAMA_TTD4 = :NAMA_' +
        'TTD4, TGL_TTD2 = :TGL_TTD2, TGL_TTD3 = :TGL_TTD3, TGL_TTD4 = :TG' +
        'L_TTD4, NIK = :NIK'
      'WHERE'
      '  NO_TRANSAKSI = :Old_NO_TRANSAKSI')
    SQLLock.Strings = (
      'SELECT * FROM ACTION_PERMINTAAN_ASET_M'
      'WITH (UPDLOCK, ROWLOCK, HOLDLOCK)'
      'WHERE'
      '  NO_TRANSAKSI = :Old_NO_TRANSAKSI')
    SQLRefresh.Strings = (
      
        'SELECT ID_TRANSAKSI, KODE, NO_TRANSAKSI, KETERANGAN, NAMA_TTD1, ' +
        'TGL_TTD1, TGL_TRANSAKSI, OPR_INSERT, TGL_INSERT, OPR_EDIT, TGL_E' +
        'DIT, OPR_TERMINATED, TGL_TERMINATED, REASON_TERMINATED, STATUS, ' +
        'OLD_STATUS, BAGIAN, NAMA_TTD2, NAMA_TTD3, NAMA_TTD4, TGL_TTD2, T' +
        'GL_TTD3, TGL_TTD4, NIK FROM ACTION_PERMINTAAN_ASET_M'
      'WHERE'
      '  NO_TRANSAKSI = :NO_TRANSAKSI')
    Connection = _DataModule.UniErp_DEPO
    SQL.Strings = (
      '/*'
      'SELECT    a.*,c.gudang,'
      '          e.NO_TRANSAKSI_D_sppb,'
      '          e.KODE_ITEM,h.NAMA_ITEM,e.QTY, h.SATUAN'
      ' ,(SELECT  d.NO_POLISI FROM sppb_m  c  '
      
        '    left outer join TB_ARIS.dbo.MST_kendaraan d on d.KODE=c.KODE' +
        '_KENDARAAN and d.DEPO=c.depo'
      '    where c.NO_TRANSAKSI=E.no_transaksi_d_sppb) as No_Pol'
      '   '
      '    ,(SELECT  d.KETERANGAN FROM sppb_m  c  '
      
        '    left outer join TB_ARIS.dbo.MST_kendaraan d on d.KODE=c.KODE' +
        '_KENDARAAN and d.DEPO=c.depo'
      '    where c.NO_TRANSAKSI=e.no_transaksi_d_sppb) as kendaraan'
      '    '
      'FROM BTB_m AS a left outer join'
      
        '     TB_ARIS.dbo.MST_LOKASI_GUDANG c on a.KODE_GUDANG=c.kode and' +
        ' a.DEPO=c.depo left outer join'
      '     BTB_D e on a.NO_TRANSAKSI=e.NO_TRANSAKSI left outer join '
      '     TB_ARIS.dbo.MST_ITEM h on e.KODE_ITEM=h.KODE_ITEM'
      ''
      '*/'
      ''
      'SELECT    a.*,'
      '          b.NO_TRANSAKSI,'
      '          b.KODE_ITEM,c.NAMA_ITEM,b.QTY, c.SATUAN'
      '   '
      'FROM ACTION_PERMINTAAN_ASET_M AS a left outer join'
      
        '     ACTION_PERMINTAAN_ASET_D b on a.NO_TRANSAKSI=b.NO_TRANSAKSI' +
        ' left outer join '
      '     TB_ARIS.dbo.MST_ITEM c on b.KODE_ITEM=c.KODE_ITEM'
      '     '
      'where  a.TGL_TRANSAKSI>=:tgl1'
      '  and  a.TGL_TRANSAKSI<=:tgl2'
      '  order by a.id_transaksi')
    Left = 457
    Top = 325
