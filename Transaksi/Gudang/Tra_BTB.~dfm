  object ZMaster: TUniQuery [11]
    SQLInsert.Strings = (
      'INSERT INTO btb_m'
      
        '  (ID_TRANSAKSI, KODE, NO_TRANSAKSI, KETERANGAN, NAMA_TTD1, TGL_' +
        'TTD1, TGL_TRANSAKSI, OPR_INSERT, TGL_INSERT, OPR_EDIT, TGL_EDIT,' +
        ' OPR_TERMINATED, TGL_TERMINATED, REASON_TERMINATED, STATUS, OLD_' +
        'STATUS, NAMA_TTD2, NAMA_TTD3, NAMA_TTD4, TGL_TTD2, TGL_TTD3, TGL' +
        '_TTD4, KODE_GUDANG, DEPO, RIT, JENIS, kode_kendaraan, kode_outle' +
        't, is_redeliver, Is_potong_AR)'
      'VALUES'
      
        '  (:ID_TRANSAKSI, :KODE, :NO_TRANSAKSI, :KETERANGAN, :NAMA_TTD1,' +
        ' :TGL_TTD1, :TGL_TRANSAKSI, :OPR_INSERT, :TGL_INSERT, :OPR_EDIT,' +
        ' :TGL_EDIT, :OPR_TERMINATED, :TGL_TERMINATED, :REASON_TERMINATED' +
        ', :STATUS, :OLD_STATUS, :NAMA_TTD2, :NAMA_TTD3, :NAMA_TTD4, :TGL' +
        '_TTD2, :TGL_TTD3, :TGL_TTD4, :KODE_GUDANG, :DEPO, :RIT, :JENIS, ' +
        ':kode_kendaraan, :kode_outlet, :is_redeliver, :Is_potong_AR)')
    SQLDelete.Strings = (
      'DELETE FROM btb_m'
      'WHERE'
      '  NO_TRANSAKSI = :Old_NO_TRANSAKSI')
    SQLUpdate.Strings = (
      'UPDATE btb_m'
      'SET'
      
        '  ID_TRANSAKSI = :ID_TRANSAKSI, KODE = :KODE, NO_TRANSAKSI = :NO' +
        '_TRANSAKSI, KETERANGAN = :KETERANGAN, NAMA_TTD1 = :NAMA_TTD1, TG' +
        'L_TTD1 = :TGL_TTD1, TGL_TRANSAKSI = :TGL_TRANSAKSI, OPR_INSERT =' +
        ' :OPR_INSERT, TGL_INSERT = :TGL_INSERT, OPR_EDIT = :OPR_EDIT, TG' +
        'L_EDIT = :TGL_EDIT, OPR_TERMINATED = :OPR_TERMINATED, TGL_TERMIN' +
        'ATED = :TGL_TERMINATED, REASON_TERMINATED = :REASON_TERMINATED, ' +
        'STATUS = :STATUS, OLD_STATUS = :OLD_STATUS, NAMA_TTD2 = :NAMA_TT' +
        'D2, NAMA_TTD3 = :NAMA_TTD3, NAMA_TTD4 = :NAMA_TTD4, TGL_TTD2 = :' +
        'TGL_TTD2, TGL_TTD3 = :TGL_TTD3, TGL_TTD4 = :TGL_TTD4, KODE_GUDAN' +
        'G = :KODE_GUDANG, DEPO = :DEPO, RIT = :RIT, JENIS = :JENIS, kode' +
        '_kendaraan = :kode_kendaraan, kode_outlet = :kode_outlet, is_red' +
        'eliver = :is_redeliver, Is_potong_AR = :Is_potong_AR'
      'WHERE'
      '  NO_TRANSAKSI = :Old_NO_TRANSAKSI')
    SQLLock.Strings = (
      'SELECT * FROM btb_m'
      'WITH (UPDLOCK, ROWLOCK, HOLDLOCK)'
      'WHERE'
      '  NO_TRANSAKSI = :Old_NO_TRANSAKSI')
    SQLRefresh.Strings = (
      
        'SELECT ID_TRANSAKSI, KODE, NO_TRANSAKSI, KETERANGAN, NAMA_TTD1, ' +
        'TGL_TTD1, TGL_TRANSAKSI, OPR_INSERT, TGL_INSERT, OPR_EDIT, TGL_E' +
