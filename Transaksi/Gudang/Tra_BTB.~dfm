  object Browse_Permintaan: TUniQuery
    Connection = _DataModule.UniErp_DEPO
    SQL.Strings = (
      
        'SELECT    a.*,b.NAMA as nama_penanggung_jawab,c.gudang,d.NO_POLI' +
        'SI,d.KETERANGAN as kendaraan,'
      '          e.NO_TRANSAKSI_NOTA,E.ID_TRANSAKSI_D,'
      
        '          e.KODE_ITEM,h.NAMA_ITEM,e.QTY, h.SATUAN,i.kd_outlet,f.' +
        'nama as nama_outlet'
      'FROM sppb_m AS a left outer join'
      
        '     TB_HRIS.dbo.M_KARYAWAN b on a.KODE_PENANGGUNG_JAWAB=b.NIK a' +
        'nd a.DEPO=b.KODE left outer join'
      
        '     TB_ARIS.dbo.MST_LOKASI_GUDANG c on a.KODE_GUDANG=c.kode and' +
        ' a.DEPO=c.depo left outer join'
      
        '     TB_ARIS.dbo.MST_kendaraan d on a.KODE_KENDARAAN=d.kode and ' +
        'a.DEPO=d.depo left outer join'
      '     SPPB_D e on a.NO_TRANSAKSI=e.NO_TRANSAKSI left outer join '
      
        '     TB_ARIS.dbo.MST_ITEM h on e.KODE_ITEM=h.KODE_ITEM left oute' +
        'r join'
      
        '     View_Nota_all  i on i.no_transaksi=e.no_transaksi_nota left' +
        ' outer join'
      '     '
      '     Mst_outlet f on f.kode=i.kd_outlet and f.depo=a.depo'
      'where a.STATUS='#39'2'#39'  '
      '  and A.jenis='#39'KIRIMAN'#39
      '  and a.tgl_transaksi>=:0'
      '  and a.tgl_transaksi<=:1'
      'order by a.id_transaksi')
    Left = 265
    Top = 389
