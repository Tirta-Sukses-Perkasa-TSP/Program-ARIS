        'DE_ITEM=a.kode_item) as NAMA_ITEM'
      
        '  --,(select b.SATUAN from  TB_ARIS.dbo.MST_ITEM b where b.KODE_' +
        'ITEM=a.kode_item) as SATUAN'
      '  ,(SELECT  d.NO_POLISI FROM sppb_m  c  '
      
        '    left outer join TB_ARIS.dbo.MST_kendaraan d on d.KODE=c.KODE' +
        '_KENDARAAN and d.DEPO=c.depo'
      '    where c.NO_TRANSAKSI=a.no_transaksi_d_sppb) as No_Pol'
      '   '
      '    ,(SELECT  d.KETERANGAN FROM sppb_m  c  '
      
        '    left outer join TB_ARIS.dbo.MST_kendaraan d on d.KODE=c.KODE' +
        '_KENDARAAN and d.DEPO=c.depo'
      '    where c.NO_TRANSAKSI=a.no_transaksi_d_sppb) as kendaraan'
      '    '
      '  from btb_d  a'
      ''
      'where no_transaksi=:no_transaksi')
