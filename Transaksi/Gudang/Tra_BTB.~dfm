  object Zdetil: TUniQuery [16]
    KeyFields = 'id_transaksi_d'
    SQLInsert.Strings = (
      'INSERT INTO btb_d'
      
        '  (NO_TRANSAKSI, ID_TRANSAKSI_D_SPPB, KODE_ITEM, QTY, No_TRANSAK' +
        'SI_D_SPPB, kode_outlet)'
      'VALUES'
      
        '  (:NO_TRANSAKSI, :ID_TRANSAKSI_D_SPPB, :KODE_ITEM, :QTY, :No_TR' +
        'ANSAKSI_D_SPPB, :kode_outlet)'
      'SET :ID_TRANSAKSI_D = SCOPE_IDENTITY()')
    SQLDelete.Strings = (
      'DELETE FROM btb_d'
      'WHERE'
      '  ID_TRANSAKSI_D = :Old_ID_TRANSAKSI_D')
    SQLUpdate.Strings = (
      'UPDATE btb_d'
      'SET'
      
        '  NO_TRANSAKSI = :NO_TRANSAKSI, ID_TRANSAKSI_D_SPPB = :ID_TRANSA' +
        'KSI_D_SPPB, KODE_ITEM = :KODE_ITEM, QTY = :QTY, No_TRANSAKSI_D_S' +
        'PPB = :No_TRANSAKSI_D_SPPB, kode_outlet = :kode_outlet'
      'WHERE'
      '  ID_TRANSAKSI_D = :Old_ID_TRANSAKSI_D')
    SQLLock.Strings = (
      'SELECT * FROM btb_d'
      'WITH (UPDLOCK, ROWLOCK, HOLDLOCK)'
      'WHERE'
      '  ID_TRANSAKSI_D = :Old_ID_TRANSAKSI_D')
    SQLRefresh.Strings = (
      
        'SELECT NO_TRANSAKSI, ID_TRANSAKSI_D_SPPB, KODE_ITEM, QTY, No_TRA' +
        'NSAKSI_D_SPPB, kode_outlet FROM btb_d'
