  object ZBrowse: TUniQuery [13]
    Connection = _DataModule.UniErp_DEPO
    SQL.Strings = (
      
        'SELECT     a.ID_TRANSAKSI, a.NO_TRANSAKSI, a.TGL_TRANSAKSI, a.ST' +
        'ATUS,'
      '           b.no_GIRO, b.status_cair, c.NILAI, '
      '           c.KODE_BANK, c.TGL_JATUH_TEMPO, c.ATAS_NAMA, d.NAMA,'
      '           c.KD_OUTLET,e.NAMA_BANK'
      'FROM         Giro_Cair_M a INNER JOIN'
      
        '                      GIRO_CAIR_D  b ON a.NO_TRANSAKSI = b.NO_TR' +
        'ANSAKSI INNER JOIN'
      
        '                      SETORAN_D c ON b.no_GIRO = c.no_GIRO left ' +
        'outer join'
      
        '                      MST_OUTLET d ON c.KD_OUTLET = d.KODE and a' +
        '.depo = d.depo'
      
        '                      left outer join tb_hris.dbo.mst_bank  e on' +
        ' e.kode=c.KODE_BANK'
      ''
      'where  a.TGL_TRANSAKSI>=:tgl1'
      '  and  a.TGL_TRANSAKSI<=:tgl2')
    Left = 561
    Top = 285
    ParamData = <
      item
        DataType = ftDateTime
        Name = 'tgl1'
        ParamType = ptInput
        Value = 40909d
      end
      item
