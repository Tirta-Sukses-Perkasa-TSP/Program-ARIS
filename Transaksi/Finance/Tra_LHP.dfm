  object Browse_Tagihan_AR: TUniQuery
    Connection = _DataModule.UniErp_DEPO
    SQL.Strings = (
      
        'select r.NO_TRANSAKSI,SUBSTRING(r.no_transaksi,8,len(r.no_transa' +
        'ksi)-7) as no_urut,'
      '       r.KD_OUTLET,r.debet-ISNULL(r.kredit,0) as saldo,'
      
        '       r.tanggal_transaksi as tgl_transaksi,r.jenis_pembayaran a' +
        's jenis_bayar,r.tgl_jatuh_tempo,r.depo,'
      'w.NAMA as nama_outlet,w.ALAMAT_1,'
      '       w.penagih_nik,g.NAMA as nama_penagih  from ('
      
        'select a.NO_TRANSAKSI,a.KD_OUTLET, a.debet,(select SUM(h.kredit)' +
        ' from Rekap_AR h'
      
        '                                            where h.No_Nota=a.No' +
        '_Nota and h.is_nota='#39'0'#39') as kredit,'
      
        '      a.tanggal_transaksi,a.jenis_pembayaran,a.tgl_jatuh_tempo,a' +
        '.depo'
      '  from Rekap_AR a '
      
        '/*    inner join  (select b.NO_TRANSAKSI_NOTA,c.KODE_PENANGGUNG_' +
        'JAWAB  from SPPB_D b '
      '       inner join SPPB_M c on c.NO_TRANSAKSI=b.NO_TRANSAKSI'
      '       where c.STATUS='#39'2'#39' or c.STATUS='#39'4'#39
      '       group by b.NO_TRANSAKSI_NOTA,c.KODE_PENANGGUNG_JAWAB) f'
      '       on a.NO_TRANSAKSI=f.NO_TRANSAKSI_NOTA '
      '*/'
      '  where  a.jenis_pembayaran<>'#39'TUNAI'#39
      '       and a.is_nota='#39'1'#39
      '     '
      '       --and a.tgl_jatuh_tempo<=GETDATE()'
      
        '       ) r left outer join  MST_OUTLET w on w.kode=r.KD_OUTLET a' +
        'nd w.depo=r.DEPO'
      
        '           left outer join tb_hris.dbo.m_karyawan g on g.NIK=w.p' +
        'enagih_nik and g.KODE=r.DEPO '
      '       where r.debet-isnull(r.kredit,0)>0; '
      ''
      '/*'
      
        'select a.NO_TRANSAKSI,a.KD_OUTLET,a.tgl_transaksi,a.jenis_bayar,' +
        'a.tgl_jatuh_tempo,b.NAMA as nama_outlet,b.ALAMAT_1,'
      '       b.penagih_nik,c.NAMA as nama_penagih,a.status, '
      '       a.NETTO-ISNULL(a.total_bayar,0) as saldo'
      
        '  from NOTA_JUAL_M a inner join MST_OUTLET b on a.KD_OUTLET=b.KO' +
        'DE and b.depo=a.DEPO'
      
        '       left outer join tb_hris.dbo.m_karyawan c on c.NIK=b.penag' +
        'ih_nik and c.KODE=a.DEPO '
      '  where (a.STATUS='#39'2'#39' or a.STATUS='#39'4'#39')'
      '       and a.tgl_jatuh_tempo<=GETDATE()'
      '       and a.jenis_bayar<>'#39'TUNAI'#39
      '       and a.NETTO-ISNULL(a.total_bayar,0)>0'
      '*/')
    Left = 265
    Top = 309
