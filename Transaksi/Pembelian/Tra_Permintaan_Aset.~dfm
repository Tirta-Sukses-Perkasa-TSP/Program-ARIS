  object ZBrowse: TUniQuery [13]
    Connection = _DataModule.UniFirebird
    SQL.Strings = (
      'SELECT    a.*,b.kode_item,b.qty,b.qty_kirim,c.NAMA_ITEM'
      '        '
      'FROM PERMINTAAN_ASET_M AS a left outer join'
      
        '     PERMINTAAN_ASET_D b on a.NO_TRANSAKSI=b.NO_TRANSAKSI left o' +
        'uter join '
      
        '     TB_ARIS.dbo.MST_ITEM c on b.kode_item=c.kode_item left oute' +
        'r join'
      '     View_Nota_all d on a.NO_TRANSAKSI=d.NO_TRANSAKSI'
      ''
      '     '
      'where  a.TGL_TRANSAKSI>=:tgl1'
      '  and  a.TGL_TRANSAKSI<=:tgl2')
    Left = 489
    Top = 325
    ParamData = <
      item
        DataType = ftDateTime
        Name = 'tgl1'
        ParamType = ptInput
      end
      item
        DataType = ftDateTime
        Name = 'tgl2'
        ParamType = ptInput
      end>
