  object User_acc: TUniQuery
    Connection = _DataModule.UniFirebird
    SQL.Strings = (
      'select * from action_aris_user_acceptance'
      'where no_transaksi_permintaan=:no_transaksi')
    MasterSource = Datasource1
    MasterFields = 'NO_TRANSAKSI'
    DetailFields = 'No_TRANSAKSI_PERMINTAAN'
    Active = True
    BeforeInsert = ZMasterBeforeDelete
    BeforePost = ZMasterBeforeDelete
    Left = 216
    Top = 341
    ParamData = <
      item
        DataType = ftString
        Name = 'NO_TRANSAKSI'
        ParamType = ptInput
        Value = 'TB - HO-1'
      end>
    object StringField12: TStringField
      FieldName = 'NO_TRANSAKSI'
      Required = True
      Size = 50
    end
    object IntegerField2: TIntegerField
      AutoGenerateValue = arAutoInc
      FieldName = 'Id_transaksi_D'
      ReadOnly = True
      Required = True
    end
    object StringField14: TStringField
      FieldName = 'KETERANGAN'
      Size = 255
    end
    object User_accNAMA_ITEM: TStringField
      FieldName = 'NAMA_ITEM'
      Required = True
      Size = 255
    end
    object User_accNOMER_ASSET: TStringField
      FieldName = 'NOMER_ASSET'
      Size = 50
    end
    object User_accQTY: TIntegerField
      FieldName = 'QTY'
      Required = True
    end
    object User_accID_TRANSAKSI_D_PERMINTAAN: TIntegerField
      FieldName = 'ID_TRANSAKSI_D_PERMINTAAN'
    end
    object User_accNo_TRANSAKSI_PERMINTAAN: TStringField
      FieldName = 'No_TRANSAKSI_PERMINTAAN'
      Size = 50
    end
  end
  object wwDataSource1: TwwDataSource
    DataSet = User_acc
    Left = 216
    Top = 392
  end
  object ppDBPipeline3: TppDBPipeline
    DataSource = wwDataSource1
    UserName = 'DBPipeline3'
