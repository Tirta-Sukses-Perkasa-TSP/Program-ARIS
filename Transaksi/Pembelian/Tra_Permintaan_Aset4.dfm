        'STATUS = :STATUS, OLD_STATUS = :OLD_STATUS, BAGIAN = :BAGIAN, JE' +
        'NIS_PERMINTAAN = :JENIS_PERMINTAAN, NIK = :NIK, NAMA_TTD2 = :NAM' +
